{
  "name": "jsx-dsl",
  "version": "1.1.3",
  "description": "Ultra-minimal DSL that compiles to React with 70% less code. TypeScript, source maps, and VSCode support included.",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "type": "module",
  "bin": {
    "jsx-dsl": "./src/cli.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./vite": {
      "types": "./vite-plugin-jsx-dsl.d.ts",
      "import": "./vite-plugin-jsx-dsl.js"
    },
    "./webpack": {
      "types": "./jsx-dsl-loader.d.ts",
      "import": "./jsx-dsl-loader.js"
    },
    "./babel": {
      "types": "./babel-plugin-jsx-dsl.d.ts",
      "import": "./babel-plugin-jsx-dsl.js"
    },
    "./runtime": "./src/runtime.js",
    "./cli": "./src/cli.js",
    "./types": "./jsx-dsl.d.ts"
  },
  "scripts": {
    "compile": "node src/cli.js",
    "test": "node src/test.js"
  },
  "keywords": [
    "react",
    "dsl",
    "compiler",
    "jsx",
    "typescript",
    "jsx-dsl",
    "minimal"
  ],
  "author": "JSX DSL Team",
  "license": "MIT",
  "homepage": "https://github.com/jsx-dsl/jsx-dsl",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsx-dsl/jsx-dsl.git"
  },
  "bugs": {
    "url": "https://github.com/jsx-dsl/jsx-dsl/issues"
  },
  "files": [
    "src/",
    "examples/",
    "vscode-extension/",
    "vite-plugin-jsx-dsl.js",
    "vite-plugin-jsx-dsl.d.ts",
    "jsx-dsl-loader.js",
    "jsx-dsl-loader.d.ts",
    "babel-plugin-jsx-dsl.js",
    "babel-plugin-jsx-dsl.d.ts",
    "jsx-dsl.d.ts",
    "*.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "esbuild": "^0.25.10",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
